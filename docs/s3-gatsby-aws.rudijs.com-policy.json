{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Action": ["s3:ListBucket"],
      "Effect": "Allow",
      "Resource": "arn:aws:s3:::gatsby-aws.rudijs.com",
      "Principal": "*"
    },
    {
      "Action": ["s3:GetObject"],
      "Effect": "Allow",
      "Resource": "arn:aws:s3:::gatsby-aws.rudijs.com/*",
      "Principal": "*"
    }
  ]
}
